result-view {
  match: SkillsOutStructure (this) {
  }

  message {
    template("#{value(this.name)}의 추천 스킬트리 입니다.")
  }

  render {
    layout-macro (skill-Tree) {
      param(name) {
        expression (name)
      }
      param(skills) {
        expression (skills)
      }
      param(engname) {
        expression (engname)
      }
      param(version) {
        expression (version)
      }
      param(skillset) {
        expression (skillset)
      }
      param (spells) {
        expression (spells)
      }
    }
  }
  conversation-drivers {
    conversation-driver {
      template ("#{value(this.name)} 정보")
    }
    conversation-driver {
      template ("#{value(this.name)} 아이템")
    }
    conversation-driver {
      template ("#{value(this.name)} 룬")
    }
    conversation-driver {
      template ("#{value(this.name)} 카운터픽")
    }
  }
}